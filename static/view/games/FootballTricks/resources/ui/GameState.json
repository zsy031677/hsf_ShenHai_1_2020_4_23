            "parent": "enhancedScene",
            "style": "dialogButton",
            "width": 260,
            "height": 400,
            "background": {
                "image": "FinalArt/Menu/Game/SideMenu.png"
            },
            "animations": {
                "open": [
                    {
                        "animate": {
                            "actions": [
                                ["left", "+=", 260]
                            ],
                            "time": 500
                        }
                    },
                    {
                        "final": {
                            "x": 0
                        }
                    }
                ],
                "close": [
                    {
                        "animate": {
                            "actions": [
